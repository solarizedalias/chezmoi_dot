# shellcheck shell=bash
# shellcheck source=/dev/null
if [ -f ~/.profile ]; then
  . ~/.profile
fi

if [ -f ~/.bashrc ]; then
  . ~/.bashrc
fi

if [ -f ~/.bash_only_function ]; then
  . ~/.bash_only_function
fi


if [ -f ~/.bash_function ]; then
  . ~/.bash_function
fi

export PATH="$HOME/.cargo/bin:$PATH"

